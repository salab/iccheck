{
	"name": "iccheck-client",
	"description": "ICCheck - Inconsistency Check",
	"license": "MIT",
	"version": "0.3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/salab/iccheck.git"
	},
	"engines": {
		"vscode": "^1.90.0"
	},
	"activationEvents": [
		"onLanguage"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b && cd ../../ && go build . && cp iccheck ./plugins/vscode",
		"watch": "cd ../../ && npm-watch",
		"dev": "code --extensionDevelopmentPath=$(pwd)"
	},
	"dependencies": {
		"vscode-languageclient": "9.0.1"
	},
	"devDependencies": {
		"@types/node": "20.14.5",
		"@types/vscode": "1.90.0",
		"npm-watch": "0.13.0",
		"typescript": "5.4.5"
	}
}
